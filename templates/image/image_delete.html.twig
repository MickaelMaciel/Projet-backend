{% extends "_template.html.twig" %}

{% block title %}suppression de {{image.url}}{% endblock %}

{% block content %}
   
    <div class="section">
        <h1 class="title">Suppression du image {{image.url}}</h1>


        {{form_start(confirmation_form)}}

            {{form_row(confirmation_form.confirmation,{
                label: 'je confirme la suppression du image : ' ~ image.url ~ '.'
            })}}

            <button type="submit" class="button is-danger">Supprimer</button>


        {{form_end(confirmation_form)}}
    </div>
{% endblock %}